<p id="notice"><%= notice %></p>

<h1>Listing Subscriptions</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Event</th>
      <th>Attendee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscriptions.each do |subscription| %>
      <tr>
        <td><%= subscription.event_id %></td>
        <td><%= subscription.attendee_id %></td>
        <td><%= link_to 'Show', subscription, :class => "btn btn-info"  %></td>
        <td><%= link_to 'Edit', edit_attendee_path(subscription), :class => "btn btn-warning" %></td>
        <td><%= link_to 'Destroy', subscription, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscription', new_subscription_path %>
